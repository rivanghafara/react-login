     <Form.Group id=\"itemName\">\n              <Form.Label>Item Name</Form.Label>\n              <Form.Control type=\"text\" id=\"item_name\" defaultValue={payload.item_name} onChange={e => setPayload({ ...payload, item_name: e.target.value })} />\n            </Form.Group>\n            <Form.Group id=\"text\">\n              <Form.Label>Description</Form.Label>\n              <Form.Control as=\"textarea\" rows={3} defaultValue={payload.item_desc} onChange={e => setPayload({ ...payload, item_desc: e.target.value })} />\n            </Form.Group>\n            <Form.Group id=\"priceTag\">\n              <Form.Label>Item Price</Form.Label>\n              <Form.Control type=\"number\" id=\"item_price\" defaultValue={payload.item_price} onChange={e => setPayload({ ...payload, item_price: e.target.value })} required />\n            </Form.Group>\n            <Button disable={loading.toString()} className=\"w-100\" type=\"submit\">Save Item</Button>\n          </Form>\n          <div className=\"w-100 text-center mt-2\">\n            <Link to=\"/\">Cancel</Link>\n          </div>\n        </Card.Body>\n      </Card>\n    </>\n  )\n}","/Volumes/Devrianza/Personal/Project/react-login/src/components/Navbar.jsx",["90","91","92","93"],{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":2,"message":"99","line":5,"column":20,"nodeType":"100","endLine":5,"endColumn":26},{"ruleId":"98","severity":2,"message":"99","line":6,"column":23,"nodeType":"100","endLine":6,"endColumn":29},{"ruleId":"98","severity":2,"message":"99","line":7,"column":35,"nodeType":"100","endLine":7,"endColumn":41},{"ruleId":"101","severity":1,"message":"102","line":13,"column":10,"nodeType":"100","messageId":"103","endLine":13,"endColumn":15},{"ruleId":"101","severity":1,"message":"104","line":13,"column":17,"nodeType":"100","messageId":"103","endLine":13,"endColumn":25},{"ruleId":"101","severity":1,"message":"105","line":2,"column":31,"nodeType":"100","messageId":"10
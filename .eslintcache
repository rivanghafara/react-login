shboard.jsx",["87","88","89","90","91","92"],"import React, { useEffect, useState } from 'react'\nimport { Card, Button, Alert, CardGroup, Row, Col } from 'react-bootstrap'\nimport { Link, useHistory } from 'react-router-dom'\nimport { useAuth } from '../contexts/AuthContext'\nimport { db } from '../firebase'\nimport Cards from './Cards'\nimport MenuItem from './MenuItem'\n\nexport default function Dashboard() {\n  const [error, setError] = useState(\"\")\n  const [items, setItems] = useState([])\n  const { currentUser, logout } = useAuth()\n  const history = useHistory()\n\n  useEffect(() => {\n    db.collection('items').onSnapshot(snapshot => (\n      setItems(snapshot.docs.map(doc => ({\n        id: doc.id,\n        item_name: doc.data().item_name,\n        item_desc: doc.data().item_desc,\n        item_price: doc.data().item_price\n      })))\n    ))\n  }, [])\n\n  async function HandleLogout() {\n    setError(\"\")\n    try {\n      await logout()\n      history.push('/login')\n    } catch (error) {\n      setError('Failed to logout')\n    }\n  }\n  return (\n    <>\n      <Card>\n        <Card.Body>\n          <h2 className=\"text-center mb-4\">Profile</h2>\n          {error && <Alert variant=\"danger\">{error}</Alert>}\n          <strong>Email:</strong> {currentUser.email}\n          <Link to=\"update-profile\" className=\"btn btn-primary w-100 mt-3\"> Update Profile </Link>\n        </Card.Body>\n      </Card>\n      <div className=\"w-100 text-center mt-2\">\n        <Button variant=\"link\" onClick={HandleLogout}>Log out</Button>\n      </div>\n    </>\n  )\n}\n","/Volumes/Devrianza/Personal/Project/react-login/src/components/Login.jsx",[],"/Volumes/Devrianza/Personal/Project/react-login/src/components/PrivateRoute.jsx",[],"/Volumes/Devrianza/Personal/Project/react-login/src/components/ForgotPassword.jsx",[],"/Volumes/Devrianza/Personal/Project/react-login/src/components/UpdateProfile.jsx",[],"/Volumes/Devrianza/Personal/Project/react-login/src/components/AddItem.jsx",[],"/Volumes/Devrianza/Personal/Project/react-login/src/components/Forms.jsx",[],["93","94"],"/Volumes/Devrianza/Personal/Project/react-login/src/components/Tables.jsx",["95","96","97","98","99"],"/Volumes/Devrianza/Personal/Project/react-login/src/components/EditItem.jsx",["100"],"import React, { useState } from 'react'\nimport { Form, Button, Card, Alert } from 'react-bootstrap'\nimport { Link, useParams, useHistory } from 'react-router-dom'\nimport { useAuth } from '../contexts/AuthContext'\nimport { db } from '../firebase'\n\nexport default function EditItem(props) {\n  let { id } = useParams()\n  const history = useHistory()\n  const [loading, setLoading] = useState(false)\n  const [error, setError] = useState('')\n  const [message, setMessage] = useState('')\n  const [payload, setPayload] = useState({\n    item_name: props.location.itemsProps.item_name,\n    item_price: props.location.itemsProps.item_price,\n    item_desc: props.location.itemsProps.item_desc\n  })\n  const { currentUser } = useAuth()\n  \n  const handleSubmit = async (e) => {\n    e.preventDefault()\n    setLoading(true)\n    setMessage('')\n    setError('')\n    try {\n      await db.collection('items').doc(props.location.itemsProps.id).update(payload)\n      setMessage(\"Data has been saved\")\n      history.push(\"/manage/menu-item\")\n    } catch (error) {\n      setError(\"Failed to save data\")\n    } finally {\n      setLoading(false)\n    }\n  }\n\n  return (\n    <>\n      <Card>\n        <Card.Body>\n          <h2 className=\"text-center mb-4\">Edit item</h2>\n          {error && <Alert variant=\"danger\">{error}</Alert>}\n          {message && <Alert variant=\"success\">{message}</Alert>}\n          <Form onSubmit={handleSubmit}>\n            <Form.Group id=\"userName\">\n              <Form.Label>Employee Name</Form.Label>\n              <Form.Control type=\"text\" disabled placeholder={currentUser.email} />\n            </Form.Group>\n            <Form.Group id=\"itemName\">\n              <Form.Label>Item Name</Form.Label>\n              <Form.Control type=\"text\" id=\"item_name\" defaultValue={payload.item_name} onChange={e => setPayload({ ...payload, item_name: e.target.value })} />\n            </Form.Group>\n            <Form.Group id=\"text\">\n              <Form.Label>Description</Form.Label>\n              <Form.Control as=\"textarea\" rows={3} defaultValue={payload.item_desc} onChange={e => setPayload({ ...payload, item_desc: e.target.value })} />\n            </Form.Group>\n            <Form.Group id=\"priceTag\">\n              <Form.Label>Item Price</Form.Label>\n              <Form.Control type=\"number\" id=\"item_price\" defaultValue={payload.item_price} onChange={e => setPayload({ ...payload, item_price: e.target.value })} required />\n            </Form.Group>\n            <Button disabled={loading} className=\"w-100\" type=\"submit\">Save Item</Button>\n          </Form>\n          <div className=\"w-100 text-center mt-2\">\n            <Link to=\"/\">Cancel</Link>\n          </div>\n        </Card.Body>\n      </Card>\n    </>\n  )\n}","/Volumes/Devrianza/Personal/Project/react-login/src/components/Cards.jsx",["101","102","103","104"],"/Volumes/Devrianza/Personal/Project/react-login/src/components/Navbars.jsx",["105","106","107","108","109"],"/Volumes/Devrianza/Personal/Project/react-login/src/components/MenuItem.jsx",["110","111","112","113"],"/Volumes/Devrianza/Personal/Project/react-login/src/components/MyProfile.jsx",["114","115","116","117"],{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","severity":2,"message":"123","line":5,"column":20,"nodeType":"124","endLine":5,"endColumn":26},{"ruleId":"122","severity":2,"message":"123","line":6,"column":23,"nodeType":"124","endLine":6,"endColumn":29},{"ruleId":"122","severity":2,"message":"123","line":7,"column":35,"nodeType":"124","endLine":7,"endColumn":41},{"ruleId":"125","severity":1,"message":"126","line":2,"column":31